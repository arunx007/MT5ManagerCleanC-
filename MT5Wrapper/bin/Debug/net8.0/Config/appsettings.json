{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "MT5Wrapper": "Debug"
    }
  },
  "MT5Wrapper": {
    "Environment": "Development",
    "Version": "1.0.0",

    "MultiTenant": {
      "Enabled": true,
      "DefaultManagerId": "default",
      "ManagerHeaderName": "X-Manager-ID"
    },

    "Authentication": {
      "ManagerTokenExpiryDays": 30,
      "ClientTokenExpiryDays": 0,
      "Jwt": {
        "SecretKey": "your-super-secret-key-change-this-in-production-make-it-at-least-32-characters-long",
        "Issuer": "MT5Wrapper",
        "Audience": "MT5Wrapper-Users"
      }
    },

    "MT5": {
      "ManagerDllPath": "Libs/MetaQuotes.MT5ManagerAPI64.dll",
      "ConnectionTimeout": 10000,
      "RetryAttempts": 3,
      "PumpModes": [
        "PUMP_MODE_SYMBOLS",
        "PUMP_MODE_GROUPS",
        "PUMP_MODE_USERS"
      ]
    },

    "Cache": {
      "Redis": {
        "ConnectionString": "localhost:6379",
        "InstanceName": "MT5Wrapper"
      },
      "DefaultExpirationMinutes": 30
    },

    "AWS": {
      "Region": "us-east-1",
      "Environment": "dev"
    },

    "Security": {
      "RequireHttps": false,
      "TokenValidation": {
        "ValidateIssuer": true,
        "ValidateAudience": true,
        "ValidateLifetime": true,
        "ValidateIssuerSigningKey": true
      }
    },

    "RateLimiting": {
      "EnableRateLimiting": true,
      "RequestsPerMinute": 1000,
      "BurstLimit": 100
    },

    "CORS": {
      "AllowedOrigins": [
        "http://localhost:3000",
        "https://yourdomain.com"
      ]
    }
  }
}