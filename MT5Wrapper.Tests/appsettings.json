{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning",
      "MT5Wrapper": "Debug"
    }
  },
  "MT5Wrapper": {
    "Environment": "Testing",
    "Version": "1.0.0",
    "MultiTenant": {
      "Enabled": true,
      "DefaultManagerId": "test_mgr_001",
      "ManagerHeaderName": "X-Manager-ID"
    },
    "Authentication": {
      "ManagerTokenExpiryDays": 30,
      "ClientTokenExpiryDays": 0,
      "Jwt": {
        "SecretKey": "test-secret-key-for-testing-purposes-only-not-for-production",
        "Issuer": "MT5Wrapper-Test",
        "Audience": "MT5Wrapper-Test-Users"
      }
    },
    "MT5": {
      "ManagerDllPath": "Libs/MetaQuotes.MT5ManagerAPI64.dll",
      "ConnectionTimeout": 15000,
      "RetryAttempts": 3,
      "PumpModes": [
        "PUMP_MODE_SYMBOLS",
        "PUMP_MODE_GROUPS",
        "PUMP_MODE_USERS"
      ]
    },
    "Cache": {
      "Redis": {
        "ConnectionString": "localhost:6379",
        "InstanceName": "MT5Wrapper-Test"
      },
      "DefaultExpirationMinutes": 5
    }
  }
}